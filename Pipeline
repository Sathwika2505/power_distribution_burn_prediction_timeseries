pipeline{
    agent any

    stages{
        stage("Extract Data"){
            steps{
                sh 'python3 data_extraction.py'
            }
        }
        stage("Data Analysis"){
            steps{
                sh 'python3 data_analysis.py'
            }
        }
        stage("Data Visualization"){
            steps{
                sh 'python3 datavisualization.py'
            }
        }
        stage("Feature Engineering"){
            steps{
                sh 'python3 feature_engineering.py'
            }
        }
    }
    post{
       
        always {
            archiveArtifacts artifacts: 'cleaned_power_distribution_data.csv', onlyIfSuccessful: true
        }
            }
}